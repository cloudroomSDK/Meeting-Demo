<Window x:Class="Meeting_WPF.updateToken"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Token即将失效,请及时更新Token！" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="133*"/>
            <ColumnDefinition Width="159*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="4*"/>
        </Grid.RowDefinitions>
        <Label Content="新的Token信息:" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" Margin="0,0,10,0" VerticalAlignment="Center"/>
        <TextBox Name="textToken" Grid.Row="1" Grid.Column="0" VerticalAlignment="Stretch"  Margin="0,0,0,0" TextWrapping="Wrap" Text="" Grid.ColumnSpan="2"/>
        <Button Content="退出" HorizontalAlignment="Center" Margin="29,5,0,7" Grid.Column="0" Grid.Row="2" VerticalAlignment="Center" Width="75" Height="25" Click="Cancel_Click"/>
        <Button Content="更新Token" HorizontalAlignment="Center" Margin="29,5,0,5" Grid.Column="1" Grid.Row="2" VerticalAlignment="Center" Width="75" Height="25" Click="Ok_Click"/>
    </Grid>
</Window>
